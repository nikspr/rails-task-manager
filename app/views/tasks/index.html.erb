<div class="container p-3">
<h1>To-Do List</h1>
<% @tasks.each do |task| %>
<div class="form-check">
  <input class="form-check-input" type="checkbox" value="<%= task.title %>">
  <label class="form-check-label" for="flexCheckDefault">
    <%= link_to task.title, task_path(task) %>
  </label>
</div>
<% end %>
</div>
